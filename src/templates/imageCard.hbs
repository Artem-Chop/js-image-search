{{#each this}}
  <div class='photo-card'>

    <a href='{{this.largeImageURL}}'><img
        src='{{this.webformatURL}}'
        alt='{{this.tags}}'
        loading='lazy'
      /></a>
    <div class='info'>
      <p class='info-item'>{{this.likes}}
        <b>Likes</b>
      </p>
      <p class='info-item'>{{this.views}}
        <b>Views</b>
      </p>
      <p class='info-item'>{{this.comments}}
        <b>Comments</b>
      </p>
      <p class='info-item'>{{this.downloads}}
        <b>Downloads</b>
      </p>
    </div>
  </div>
{{/each}}